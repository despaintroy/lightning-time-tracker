{
  "name": "lk-react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "npm run build:prod",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "deploy": "export AWS_PROFILE=lk || set AWS_PROFILE=lk && npm run build && terraform -chdir=terraform/lightningkite init && terraform -chdir=terraform/lightningkite apply -auto-approve",
    "serve": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@lightningkite/lightning-server-simplified": "^1.0.1",
    "@lightningkite/mui-lightning-components": "^1.1.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/lab": "^5.0.0-alpha.136",
    "@mui/material": "^5.14.0",
    "@mui/x-data-grid": "^5.17.26",
    "@mui/x-date-pickers": "^5.0.20",
    "@ngneat/falso": "^6.4.0",
    "dayjs": "^1.11.9",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "formik": "^2.4.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^18.16.19",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vite-plugin-checker": "^0.6.1",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
